<div class="add-inventory">
  <% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>
  
  <div id="flash-message" class="flash-message">
    <%- messages() %>
  </div>
  
  <% if (errors.length > 0) { %>
    <ul class="notice">
      <% errors.forEach(error => { %>
        <li><%= error.msg %></li>
      <% }) %>
    </ul>
  <% } %>
  
  <%- addInventory %>
  </div>