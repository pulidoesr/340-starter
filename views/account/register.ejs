<% if (title) { %>
  <h1><%= title %></h1>
  <% } else {
    res.redirect('/')
  } %>

<div id="flash-message" class="flash-message">
  <%- messages() %>
</div>

<% if (errors) { %>
<ul class="notice">
  <% errors.array().forEach(error => { %>
  <li><%= error.msg %></li>
<% }) %>
</ul>
<% } %>

<%- registerPage %>